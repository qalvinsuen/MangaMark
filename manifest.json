{
	"name":"MangaMark",
	"version":"1.0",
	"manifest_version": 2,

	"content_scripts": [
		{
			"matches":["https://www.mangareader.net/*","https://mangahub.io/*","https://mangapark.net/*","https://www.webtoons.com/*"],
			"js":["content.js"]
		}
	],

	"browser_action":{
		"default_popup": "bookmarks.html",
		"default_icon": "mangamark.png",
		"default_title": "MangaMark"
	},

	"background": {
		"scripts": ["background.js"]
	}

}



	